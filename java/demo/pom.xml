<?xml version="1.0"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://maven.apache.org/POM/4.0.0"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <artifactId>demo</artifactId>
  <build>
    <!-- for hot reload of the web application-->
    <outputDirectory>
      ${project.build.directory}/${project.build.finalName}/WEB-INF/classes
    </outputDirectory>
    <plugins>
      <plugin>
        <artifactId>appengine-maven-plugin</artifactId>
        <groupId>com.google.cloud.tools</groupId>
        <version>2.4.4</version>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>11</source>
          <target>11</target>
        </configuration>
        <groupId>org.apache.maven.plugins</groupId>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <!-- Compile/runtime dependencies -->

    <dependency>
      <artifactId>soy</artifactId>
      <groupId>com.google.template</groupId>
      <version>2022-07-20</version>
    </dependency>
    <dependency>
      <artifactId>appengine-api-1.0-sdk</artifactId>
      <groupId>com.google.appengine</groupId>
      <version>2.0.8</version>
    </dependency>
    <dependency>
      <artifactId>servlet-api</artifactId>
      <groupId>javax.servlet</groupId>
      <scope>provided</scope>
      <type>jar</type>
      <version>2.5</version>
    </dependency>
    <dependency>
      <artifactId>commons-io</artifactId>
      <groupId>commons-io</groupId>
      <version>2.7</version>
    </dependency>
    <dependency>
      <artifactId>commons-fileupload</artifactId>
      <groupId>commons-fileupload</groupId>
      <version>1.4</version>
    </dependency>
    <dependency>
      <artifactId>commons-lang</artifactId>
      <groupId>commons-lang</groupId>
      <version>2.6</version>
    </dependency>
    <dependency>
      <artifactId>libphonenumber</artifactId>
      <groupId>com.googlecode.libphonenumber</groupId>
      <version>8.12.57-SNAPSHOT</version>
    </dependency>
    <dependency>
      <artifactId>geocoder</artifactId>
      <groupId>com.googlecode.libphonenumber</groupId>
      <version>2.193-SNAPSHOT</version>
    </dependency>
    <dependency>
      <artifactId>carrier</artifactId>
      <groupId>com.googlecode.libphonenumber</groupId>
      <version>1.183-SNAPSHOT</version>
    </dependency>
  </dependencies>
  <modelVersion>4.0.0</modelVersion>
  <packaging>war</packaging>
  <parent>
    <artifactId>libphonenumber-parent</artifactId>
    <groupId>com.googlecode.libphonenumber</groupId>
    <version>8.12.57-SNAPSHOT</version>
  </parent>

  <prerequisites>
    <maven>3.5</maven>
  </prerequisites>

  <properties>
    <app.deploy.project>libphonenumber-hrd</app.deploy.project>
    <archiveClasses>true</archiveClasses>
    <maven.compiler.showDeprecation>true</maven.compiler.showDeprecation>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
  </properties>

  <url>https://github.com/google/libphonenumber/</url>

  <version>8.12.57-SNAPSHOT</version>
</project>
